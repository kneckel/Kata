function sort(items){
  //items.sort();
  var endWhile = false;
  while (!endWhile) {
    endWhile = true;
  for(var i=1; i < items.length; i++){
    if(items[i-1]>items[i]){
    endWhile = false;
      var temparr = items[i-1];
      items[i-1] = items[i];
      items[i] = temparr;
    }
    }
  }
  return items;
  
}
